{
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    }
  ],

  "redirects": [
    {
      "source": "/",
      "destination": "/manual/docs-home.html",
      "permanent": true
    }
  ],
  "build": {
    "ignoreCommand": "git diff --quiet HEAD^ HEAD ./docs"
  }
}
